void can_msg_handler(void)
{
	pwm_handler();
	output_handler();
	button_handler();
}

int main(void)
{
	uint8_t i;

	/* disable watchdog timer */
	MCUSR &= ~(_BV(WDRF) | _BV(BORF) | _BV(EXTRF) | _BV(PORF));
	wdt_disable();

	/* module initalization */
	pwm_init();
	output_init();
	button_init();
	uart_init();

  tick_init();

	printf("power on\n");

	/* INT0, low level */
	EIMSK = _BV(INT0);

	mcp2515_init();

	/* power saving mode */
	set_sleep_mode(SLEEP_MODE_IDLE);
